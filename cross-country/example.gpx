<?xml version='1.0' encoding='UTF-8' standalone='no' ?>
<gpx xmlns="http://www.topografix.com/GPX/1/1" version="1.1" creator="OpenRally.org"
	xmlns:cc="http://www.openrally.org/xmlschemas/GpxExtensions/v1.0-Cross-Country-DRAFT">
	<metadata>
		<extensions>
			<cc:units>metric</cc:units>
			<cc:distance>33.86</cc:distance>
		</extensions>
	</metadata>

  <!-- minimum <wpt> must have a distance -->
	<wpt lat="0" lon="0"> <extensions> <cc:distance>0.0</cc:distance> </extensions> </wpt>

	<!-- speed zones -->
	<wpt lat="0" lon="0">
		<extensions>
			<cc:distance>0.1</cc:distance>
			<cc:dz/>
			<cc:speed>65</cc:speed>
		</extensions>
	</wpt>
	<wpt lat="0" lon="0">
		<extensions>
			<cc:distance>0.2</cc:distance>
			<cc:fz/>
		</extensions>
	</wpt>

  <!-- danger !!! with WPS as per FIM -->
	<wpt lat="0" lon="0">
		<extensions>
			<cc:distance>0.3</cc:distance>
			<cc:danger>3</cc:danger>
			<cc:wps open='800' clear='90'/>
		</extensions>
	</wpt>

	<!-- Stop for 3 seconds at road crossing -->
	<wpt lat="0" lon="0">
		<extensions>
			<cc:distance>0.4</cc:distance>
			<cc:stop>3</cc:stop>
		</extensions>
	</wpt>

  <!-- Neutralized fuel stop, 15 mins (900 seconds) -->
	<wpt lat="0" lon="0">
		<extensions>
			<cc:distance>0.5</cc:distance>
			<cc:neutralization>600</cc:neutralization>
			<cc:fuel/>
		</extensions>
	</wpt>

  <!-- Tulip and Notes drawings -->
	<wpt lat="0" lon="0">
		<extensions>
			<cc:distance>0.6</cc:distance>
			<cc:danger>2</cc:danger>
			<cc:tulip>
				<![CDATA[
				<svg>
					<img href="data:image/png;base64,R0lGODlhAQABAIAAAP///wAAACH5BAEAAAAALAAAAAABAAEAAAICRAEAOw=="/>
				</svg>
				]]>
			</cc:tulip>
			<cc:notes>
				<![CDATA[
				<svg>
					<img href="data:image/png;base64,R0lGODlhAQABAIAAAP///wAAACH5BAEAAAAALAAAAAABAAEAAAICRAEAOw=="/>
					<text>Massive Erosion</text>
				</svg>
				]]>
			</cc:notes>
		</extensions>
	</wpt>

	<!-- or any other tags (using them all at once is nonsensical) -->
	<wpt lat="0" lon="0">
		<extensions>
			<cc:distance>0.35</cc:distance>
			<cc:wpc clear='20' name="ABC"/>
			<cc:wpm open='800' clear='200' name="XYZ"/>
			<cc:wps open='800' clear='20' name="K99"/>
			<cc:wpe clear='200'/>
			<cc:dss/>
			<cc:checkpoint/>
			<cc:ass/>
			<cc:timecontrol/>
			<cc:cap>360</cc:cap>
		</extensions>
	</wpt>
</gpx>
